"#toastWindow": {
    backgroundColor: 'transparent'
}

"#toastContainer": {
    width: 120,
    height: 120,
    borderRadius: 10,
    backgroundColor: '#000',
    zIndex: 70,
    opacity: 0
}

".toastIconWrapper": {
    height: 75,
    top: 2
}

"#toastIcon": {
    width: 35,
    height: 35
}

"#toastMessage": {
    top: 68,
    color: '#fff',
    height: 40,
    font: {
        fontSize: 12
    },
    left: 10,
    right: 10,
    textAlign: Titanium.UI.TEXT_ALIGNMENT_CENTER
}
